{{#if items}}
    <table class="table">
        <thead>
            <tr>
                {{!-- <th>#</th> --}}
                <th>診所名稱</th>
                <th>距離(m)</th>
                <th></th>
            </tr>
        </thead>
        {{#each items}}
            <tr>
                {{!-- <th>{{@index}}</th> --}}
